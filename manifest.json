{
  "name": "Fun Cat Facts",
  "version": "1.0",
  "description": "New cat facts.",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/popup.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "48": "images/img128.png",
      "128": "images/img128.png"
    }
  },
  "icons": {
    "48": "images/img128.png",
    "128": "images/img128.png"
  },
  "permissions": ["<all_urls>"]
}
